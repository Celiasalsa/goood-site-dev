{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "array-callback-return": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 1,
    "max-len": 0,
    },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
}
